{
  "name": "image-compressor",
  "productName": "ImageCompressor",
  "version": "1.0.0",
  "description": "This is a desktop application to shrink the size of image.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "dist:linux": "electron-builder --linux"
  },
  "repository": "https://github.com/surajkarki66/image-compressor.git",
  "author": "surajkarki66 <suraj.karki500@protonmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "devDependencies": {
    "electron": "^15.0.0",
    "electron-builder": "^22.11.7",
    "nodemon": "^2.0.13"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "imagemin": "7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "slash": "3.0.0"
  },
  "build": {
    "appId": "com.initdevelops.imagecompressor",
    "productName": "ImageCompressor",
    "linux": {
